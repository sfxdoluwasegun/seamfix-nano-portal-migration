<?xml version="1.0"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" version="2.0">
	<portlet>
		<portlet-name>nano-portal</portlet-name>
		<display-name>nano-portal</display-name>
		<portlet-class>com.liferay.util.bridges.mvc.MVCPortlet</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/view.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
		</supports>
		<portlet-info>
			<title>nano-portal</title>
			<short-title>nano-portal</short-title>
			<keywords>nano-portal</keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	<portlet>
		<portlet-name>system-settings</portlet-name>
		<display-name>System Settings</display-name>
		<portlet-class>
			com.nano.portal.portlets.systemsettings.SystemSettings
		</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/html/systemsettings/view.jsp</value>
		</init-param>
        <init-param>
            <name>add-process-action-success-action</name>
            <value>false</value>
        </init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<resource-bundle>content.Language</resource-bundle>
		<portlet-info>
			<title>System Settings</title>
			<short-title>System Settings</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	<portlet>
		<portlet-name>pool-management</portlet-name>
		<display-name>Pool Management</display-name>
		<portlet-class>
			com.nano.portal.portlets.poolmanagement.PoolManagement
		</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/html/poolmanagement/view.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<resource-bundle>content.Language</resource-bundle>
		<portlet-info>
			<title>Pool Management</title>
			<short-title>Pool Management</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	<portlet>
		<portlet-name>support</portlet-name>
		<display-name>Support</display-name>
		<portlet-class>
			com.nano.portal.portlets.support.Support
		</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/html/support/view.jsp</value>
		</init-param>
		<init-param>
            <name>add-process-action-success-action</name>
            <value>false</value>
        </init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<resource-bundle>content.Language</resource-bundle>
		<portlet-info>
			<title>Support</title>
			<short-title>Support</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	<filter>    
	    <filter-name>CDIPortletFilter</filter-name>    
	    <filter-class>com.liferay.cdi.portlet.bridge.CDIPortletFilter</filter-class>
	    <lifecycle>ACTION_PHASE</lifecycle>
	    <lifecycle>EVENT_PHASE</lifecycle>
	    <lifecycle>RENDER_PHASE</lifecycle>
	    <lifecycle>RESOURCE_PHASE</lifecycle>
	</filter>
	<filter-mapping>
	    <filter-name>CDIPortletFilter</filter-name>    
	    <portlet-name>system-settings</portlet-name>
	    <portlet-name>pool-management</portlet-name>
	    <portlet-name>support</portlet-name>
	</filter-mapping>
</portlet-app>